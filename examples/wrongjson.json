{
    "name": "Licht",
    "colors": {
      "editor.background": "#fffefc",
      "editor.findMatchBackground": "#C2E8FF",
      "editor.findMatchHighlightBackground": "#C2E8FF50",
      "editor.foreground": "#111111",
      "editor.inactiveSelectionBackground": "#EDEDED",
      "editor.lineHighlightBackground": "#A5A5A520",
      "editor.selectionBackground": "#C2E8FF",
      "editorCursor.foreground": "#3B68FC",
      "editorLineNumber.activeForeground": "#111111",
      "editorLineNumber.foreground": "#ADADAD",
      "editorWhitespace.foreground": "#E0E0E0"
    },
    "tokenColors": [
      {
        "name": "Comments",
        "scope": ["comment", "comment punctuation"],
        "settings": {
          "foreground": "#919191"
        }
      },
      {
        "name": "Comments: Preprocessor",
        "scope": "comment.block.preprocessor",
        "settings": {
          "foreground": "#ADADAD"
        }
      },
      {
        "name": "Comments: Documentation",
        "scope": ["comment.documentation", "comment.block.documentation"],
        "settings": {
          "background": "#FFFDF7",
          "foreground": "#BC670F"
        }
      },
      {
        "name": "Invalid - Deprecated",
        "scope": "invalid.deprecated",
        "settings": {
          "background": "#990000",
          "foreground": "#FFFFFF"
        }
      },
      {
        "name": "Invalid - Illegal",
        "scope": "invalid.illegal",
        "settings": {
          "foreground": "#cd3131"
        }
      },
      {
        "name": "Operators",
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#352de3"
        }
      },
      {
        "name": "Keywords",
        "scope": ["keyword", "storage"],
        "settings": {
          "foreground": "#352de3"
        }
      },
      {
        "name": "Types",
        "scope": ["storage.type", "support.type"],
        "settings": {
          "foreground": "#B4640B"
        }
      },
      {
        "name": "Language Constants",
        "scope": ["constant.language", "variable.language"],
        "settings": {
          "foreground": "#626FC9"
        }
      },
      {
        "name": "Variables",
        "scope": ["variable", "support.variable"],
        "settings": {
          "foreground": "#111111"
        }
      },
      {
        "name": "Variables: Punctuation",
        "scope": "variable punctuation",
        "settings": {
          "foreground": "#111111"
        }
      },
      {
        "name": "Functions",
        "scope": ["entity.name.function", "support.function", "entity"],
        "settings": {
          "foreground": "#284181"
        }
      },
      {
        "name": "Classes",
        "scope": [
          "entity.name.type",
          "entity.other.inherited-class",
          "support.class"
        ],
        "settings": {
          "foreground": "#bb28c7"
        }
      },
      {
        "name": "Exceptions",
        "scope": "entity.name.exception",
        "settings": {
          "foreground": "#F93232"
        }
      },
      {
        "name": "Sections",
        "scope": "entity.name.section",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Numbers",
        "scope": ["constant.numeric", "constant"],
        "settings": {
          "foreground": "#dd3c2f"
        }
      },
      {
        "name": "Punctuation",
        "scope": "punctuation",
        "settings": {
          "foreground": "#111111"
        }
      },
      {
        "name": "Strings",
        "scope": ["constant.character", "string"],
        "settings": {
          "foreground": "#00A33F"
        }
      },
      {
        "name": "Strings: Punctuation",
        "scope": "string punctuation",
        "settings": {
          "foreground": "#00A33F"
        }
      },
      {
        "name": "Strings: Escape Sequences",
        "scope": "constant.character.escape",
        "settings": {
          "background": "#FBE9AD44",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Strings: Regular Expressions",
        "scope": "string.regexp",
        "settings": {
          "foreground": "#699D36"
        }
      },
      {
        "name": "Results",
        "scope": "constant.numeric.line-number.bgr",
        "settings": {
          "foreground": "#111111"
        }
      },
      {
        "name": "Strings: Symbols",
        "scope": "constant.other.symbol",
        "settings": {
          "background": "#E8FFD5",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Object Keys",
        "scope": "string.unquoted.label.js",
        "settings": {
          "foreground": "#444444"
        }
      },
      {
        "name": "Quasi Literals",
        "scope": "punctuation.quasi.element",
        "settings": {
          "foreground": "#444444"
        }
      },
      {
        "name": "Embedded Source",
        "scope": ["string source", "text source"],
        "settings": {
          "foreground": "#434343"
        }
      },
      {
        "name": "HTML: Doctype Declaration",
        "scope": "meta.tag.sgml.doctype",
        "settings": {
          "foreground": "#7F7F7F"
        }
      },
      {
        "name": "HTML: Tags",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#0072c8"
        }
      },
      {
        "name": "HTML: Attribute Punctuation",
        "scope": "meta.tag string punctuation",
        "settings": {
          "foreground": "#00A33F"
        }
      },
      {
        "name": "HTML: Tag Punctuation",
        "scope": "punctuation.definition.tag",
        "settings": {
          "foreground": "#4F9FCF"
        }
      },
      {
        "name": "HTML: Entities",
        "scope": "constant.character.entity",
        "settings": {
          "foreground": "#111111"
        }
      },
      {
        "name": "HTML: Attribute Names",
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#0072c8"
        }
      },
      {
        "name": "HTML: Attribute Values",
        "scope": [
          "meta.tag string.quoted",
          "meta.tag string.quoted constant.character.entity"
        ],
        "settings": {
          "background": "#FFFFFF",
          "foreground": "#00A33F"
        }
      },
      {
        "name": "CSS: Selectors",
        "scope": [
          "meta.selector",
          "meta.selector entity",
          "meta.selector entity punctuation",
          "entity.name.tag.css"
        ],
        "settings": {
          "foreground": "#3A77BF"
        }
      },
      {
        "name": "CSS: Property Names",
        "scope": ["meta.property-name", "support.type.property-name"],
        "settings": {
          "foreground": "#D4430D"
        }
      },
      {
        "name": "CSS: Property Values",
        "scope": [
          "meta.property-value constant.numeric",
          "meta.property-value constant",
          "meta.property-value keyword"
        ],
        "settings": {
          "foreground": "#43A202"
        }
      },
      {
        "name": "Markup: Changed",
        "scope": "markup.changed",
        "settings": {
          "background": "#FFFFDD",
          "foreground": "#111111"
        }
      },
      {
        "name": "Markup: Deletion",
        "scope": "markup.deleted",
        "settings": {
          "background": "#FFDDDD",
          "foreground": "#111111"
        }
      },
      {
        "name": "Markup: Emphasis",
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup: Error",
        "scope": "markup.error",
        "settings": {
          "foreground": "#cd3131"
        }
      },
      {
        "name": "Markup: Insertion",
        "scope": "markup.inserted",
        "settings": {
          "background": "#DDFFDD",
          "foreground": "#111111"
        }
      },
      {
        "name": "Markup: Output",
        "scope": ["markup.output", "markup.raw"],
        "settings": {
          "foreground": "#7F7F7F"
        }
      },
      {
        "name": "Markup: Prompt",
        "scope": "markup.prompt",
        "settings": {
          "foreground": "#555555"
        }
      },
      {
        "name": "Markup: Heading",
        "scope": "markup.heading",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup: Strong",
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markup: Traceback",
        "scope": "markup.traceback",
        "settings": {
          "foreground": "#F93232"
        }
      },
      {
        "name": "Markup: Underline",
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      }
    ]


